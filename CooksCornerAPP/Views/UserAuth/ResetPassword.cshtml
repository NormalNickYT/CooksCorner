@model CooksCornerAPP.Models.DTO.ResetPassword
<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
<html>
<h1>ResetPassword</h1>

<body>
    <div class="form-contactpage">
        <form asp-action="ResetPassword">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Password" class="control-label"></label>
                <input asp-for="Password"  class="form-contactpagina__inputfield" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="ConfirmPassword" class="control-label"></label>
                <input asp-for="ConfirmPassword" class="form-contactpagina__inputfield"  class="form-control" />
            </div>
            <input type="hidden" asp-for="Email" class="form-control" />
            <input type="hidden" asp-for="Token" class="form-control" />
            <div class="form-group">
                <input type="submit" class="form-contactpagina__submit"  value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
  </body>
</html>
